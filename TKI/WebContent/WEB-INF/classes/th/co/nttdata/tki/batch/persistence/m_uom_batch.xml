<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="m_uom_batch">
	<select id="isExist" parameterClass="string" resultClass="int">
		SELECT 1
		FROM   m_uom
		WHERE  uom = #value#
	</select>
</sqlMap>